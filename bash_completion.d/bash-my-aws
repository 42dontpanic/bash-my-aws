_cf_list() {
    local command="$1"
    local word="$2"
    COMPREPLY=($(compgen -W "$(cf_list)" -- ${word}))
    return 0
}

_instances() {
    local command="$1"
    local word="$2"

    case $word in
      "") options="i-a i-b" ;;
      *)
        options=$(instances)
      ;;
    esac

    COMPREPLY=($(compgen -W "${options}" -- ${word}))
    return 0
}

complete -F _cf_list cf_elbs
complete -F _cf_list cf_instances
complete -F _cf_list cf_resources
complete -F _cf_list instances_in_stack
complete -F _instances instances
complete -F _instances instance_ip_private
complete -F _instances instance_ip_public

